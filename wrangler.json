{
    "name": "mybookai",
    "main": "worker/index.js",
    "compatibility_date": "2024-01-01",
    "site":{
        "bucket": "./dist"
    },
    "vars": {
        "ENVIRONMENT": "production"
    },
    "build": {
        "command": "npm run build"
    },
    "dev": {
        "port": 8787,
        "local_protocol": "http"
    },
    "routes": [
        {
            "pattern": "mybook.ai/*",
            "zone_id": "your_zone_id_here"
        }
    ]
}